{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "insight-canvas",
  "main": "src/worker.ts",
  "compatibility_date": "2025-09-27",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "dist",
    "binding": "ASSETS",
    "not_found_handling": "single-page-application"
  },
  "observability": {
    "enabled": true
  }
}
